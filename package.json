        "pong",
        "websockets"
    ],
    "author": "jake carr",
    "license": "ISC",
    "engines": {
        "node": "14.5.0"
    },
    "dependencies": {
        "bootstrap": "^4.6.0",
        "concurrently": "^6.2.1",
        "socket.io": "^4.2.0",
        "socket.io-client": "^4.2.0"
    },
    "devDependencies": {
        "@types/bootstrap": "^4.6.1",
        "@types/express": "^4.17.13",
        "@types/node": "^16.9.1",
        "@types/socket.io-client": "^1.4.36",
        "nodemon": "^2.0.12",
        "typescript": "^4.4.3"
    }
}
